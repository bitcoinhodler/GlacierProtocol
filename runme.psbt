#!/bin/bash

set -e

bitcoind -regtest -connect=0.0.0.0 -daemon
sleep 5

echo createwallet
bitcoin-cli -regtest -named createwallet wallet_name= descriptors=false


echo decodepsbt
bitcoin-cli -regtest decodepsbt 

echo decodescript
bitcoin-cli -regtest decodescript 522103d14ddcfb6817f5579695bbb3eb3e185887bf2942b031e6f716343b8fe7e9e8e221028fcd46f8614b2cbf318096968242a1e22bcfb6d8f2b6dc939c8c27c347b2937b210315acb550120f4cdcb460d5c49080ab508ca4ccd8dedecac22feeac8cd017d4c121022b063ee2f22f9e1982c140fe778672c683111a796dcccdbd47bb65e3d608a98354ae


echo getdescriptorinfo for first privkey
bitcoin-cli -regtest getdescriptorinfo "pkh(cQCrT9Ncs9729ao7jbmAWrD9z7tF64s2yKzmD6nkiLAi9sXVZWAn)"
#   "descriptor": "pkh(03d14ddcfb6817f5579695bbb3eb3e185887bf2942b031e6f716343b8fe7e9e8e2)#zl6jwflg",


echo getdescriptorinfo for second privkey
bitcoin-cli -regtest getdescriptorinfo "pkh(cP65UeSDZPiTLB6CBwasWv9oJYEjRgQXhswfwcT9HscEKDcEbgy4)"
#  "descriptor": "pkh(028fcd46f8614b2cbf318096968242a1e22bcfb6d8f2b6dc939c8c27c347b2937b)#5qd20afp",

# Replace the appropriate 2 of the pubkeys in the descriptor with our privkeys.


echo getdescriptorinfo to get checksum
bitcoin-cli -regtest getdescriptorinfo "wsh(multi(2,cQCrT9Ncs9729ao7jbmAWrD9z7tF64s2yKzmD6nkiLAi9sXVZWAn,cP65UeSDZPiTLB6CBwasWv9oJYEjRgQXhswfwcT9HscEKDcEbgy4,0315acb550120f4cdcb460d5c49080ab508ca4ccd8dedecac22feeac8cd017d4c1,022b063ee2f22f9e1982c140fe778672c683111a796dcccdbd47bb65e3d608a983))"

echo importmulti
bitcoin-cli -regtest importmulti '[{
  "desc": "wsh(multi(2,cQCrT9Ncs9729ao7jbmAWrD9z7tF64s2yKzmD6nkiLAi9sXVZWAn,cP65UeSDZPiTLB6CBwasWv9oJYEjRgQXhswfwcT9HscEKDcEbgy4,0315acb550120f4cdcb460d5c49080ab508ca4ccd8dedecac22feeac8cd017d4c1,022b063ee2f22f9e1982c140fe778672c683111a796dcccdbd47bb65e3d608a983))#fujqkmfr",
  "timestamp": "now",
  "watchonly": true
}]'


echo walletprocesspsbt
bitcoin-cli -regtest walletprocesspsbt  true ALL false

echo finalizepsbt
bitcoin-cli -regtest finalizepsbt 

echo decoderawtransaction
bitcoin-cli -regtest decoderawtransaction 



echo stop
bitcoin-cli -regtest stop
sleep 2
rm -rf ~/.bitcoin
